(setq font-lock-verbose nil)

(global-set-key (kbd "M-i") 'indent-region)
(global-set-key (kbd "C-<right>") 'previous-multiframe-window)
(global-set-key (kbd "C-<left>") 'next-multiframe-window)
(global-set-key (kbd "C-f") 'make-frame-command)

